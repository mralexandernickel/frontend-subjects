{
  "name": "@mralexandernickel/frontend.subjects",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "lint": "node_modules/.bin/tslint -c tslint.json --type-check --project tsconfig.json '**/*.ts' --exclude '**/node_modules/**' --exclude 'dist/**' --exclude 'amd/**'",
    "build": "yarn lint && node_modules/.bin/webpack -p && tsc",
    "build:amd": "tsc --module amd --outDir amd",
    "deploy": "cd .. && tar czf frontend-subjects.tar.gz frontend-subjects && scp frontend-subjects.tar.gz aventon:/var/www/ && ssh aventon tar xzf /var/www/frontend-subjects.tar.gz -C /var/www"
  },
  "devDependencies": {
    "@mralexandernickel/flex-attributes": "^0.5.1",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "rxjs": "^6.2.0"
  }
}
